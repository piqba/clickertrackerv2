{
  "Logging": {
    "LogLevel": {
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kafka": {
    "TopicPrefix": "events",
    "KafkaConfig": {
      "group.id": "my-id",
      "bootstrap.servers": "localhost:19092",
      "enable.auto.commit": true
    }
  }
}
